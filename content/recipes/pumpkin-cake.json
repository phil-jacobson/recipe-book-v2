    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "position": 1,
            "text": "Preheat oven to 350 degrees (325 for a glass pan)"  
        },
        {
            "@type": "HowToStep",
            "position": 2,
            "text": "Make the crust:  Mix cake mix (minus one cup, reserverd for topping) in a bowl with egg and melted butter, then press into a 9x13 inch pan."
        },
        {
            "@type": "HowToStep",
            "position": 3,
            "text": "Make the filling: Cobmine pumpkin, eggs, milk, and cinnamon.  Pour over the crust.."
        },
        {
            "@type": "HowToStep",
            "position": 4,
            "text": "Make the topping: Combine reserved cake mix with butter, sugar, and nuts.   Corumble over the filling."
        },
        {
            "@type": "HowToStep",
            "position": 5,
            "text": "Bake for  45-50 minutes."
        }
    ],
    "recipeIngredient": [
        "<h2>Crust</h2>",
        "<val>1</val> box yellow cake mix (set aside one cup)",
        "<val>1</val> egg" ,
        "<val>1/2</val> cup butter, melted",        
        "<h2>Filling</h2>",
        "<val>1</val> small can of pumpkin",
        "<val>2/3</val> cup milk",                
        "<val>2</val> tsp cinnamon",                        
        "<h2>Toping</h2>",
        "<val>1</val> cup cake mix",
        "<val>1/2</val> cup sugar",
        "<val>1/2</val> cup nuts"
    ],
