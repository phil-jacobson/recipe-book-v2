    "recipeInstructions": [        
        {
            "@type": "HowToStep",
            "position": 1,
            "text": "Dissolve sugar into the syrup and bring to a boil. "
        },
        {
            "@type": "HowToStep",
            "position": 2,
            "text": "Add peanut butter to the syrup and sugar."
        },
        {
            "@type": "HowToStep",
            "position": 3,
            "text": "Remove mixture from heat and add to a bowl with the cereal."
        },
        {
            "@type": "HowToStep",
            "position": 4,
            "text": "Press in an 8x8 buttered pan."
        },
        {
            "@type": "HowToStep",
            "position": 5,
            "text": "Mix together and melt the chocolate and butterscotch chips."
        },
        {
            "@type": "HowToStep",
            "position": 6,
            "text": "Spread melted chips on top of the Special K mixture."
        }
    ],
    "recipeIngredient": [        
        "<val>1/2</val> cup sugar ",
        "<val>1/2</val> cup light syrup ",
        "<val>4</val> cups Special K cereal (1 cup = approx 1oz)",
        "<val>1</val> cup peanut butter",
        "<val>1</val> cup butterscotch chips",
        "<val>1</val> cup chocolate chips"
    ],    
