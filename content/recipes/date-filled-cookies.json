    "recipeCategory": [
        
    ],
    "recipeInstructions": [        
        {
            "@type": "HowToStep",
            "position": 1,
            "text": "Make the filling.  Stirring constantly, cook filing ingredients until thickened.  Let cool."
        },
        {
            "@type": "HowToStep",
            "position": 2,
            "text": "Preheat oven to 375 degrees"
        },
        {
            "@type": "HowToStep",
            "position": 3,
            "text": "Blend shortening, salt, and vanilla."
        },
        {
            "@type": "HowToStep",
            "position": 4,
            "text": "Add sugar and mix thoroughly."
        },
        {
            "@type": "HowToStep",
            "position": 5,
            "text": "Sift flour with baking powder.  Slowly add to sugar and shortening mixture, along with the milk, and blend thoroughly."
        },
        {
            "@type": "HowToStep",
            "position": 6,
            "text": "Roll dough 1/8 inch thick on a floured surface, and cut with a 2 inch cookie cutter."
        },
        {
           "@type": "HowToStep",
            "position": 7,
           "text": "Place 1 tsp of filling on a cookie, place another cookie on top and press together."
        },
        {
           "@type": "HowToStep",
            "position": 8,
           "text": "Prick top of each cookie with a fork."
        },
        {
           "@type": "HowToStep",
           "position": 9,
           "text": "Place on lightly greased cookie sheet."
        },
        {
           "@type": "HowToStep",
           "position": 10,
           "text": "Bake for 15 minutes."
        }
    ],
    "recipeIngredient": [                
        "<h2>Cookie</h2>",
        "<val>1/3</val> cup spry or other shortening",
        "<val>1/2</val> tsp salt",
        "<val>1</val> cup sugar",
        "<val>3</val> cups sifted flour",
        "<val>3</val> tsp baking powderr",
        "<val>1/4</val> cup milk",
        "<h2>Filling</h2>",
        "<val>1</val> cup pitted and chopped dates",
        "<val>1/2</val> cup sugar",
        "<val>1/4</val> cup water"        
    ],    
    "totalTime": "PT2H0M"
}